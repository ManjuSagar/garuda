<form role="form" action=<%= winners_path%> method="post">
  <div class="form-group">
    <label for="WinForm">Winner For Date</label>
    <input type="datetime-local" class="form-control" style="width: 50%" name="datetime-local" > 
    <!-- value=<%= DateTime.now%> -->
    <br/>
    <label for="WinForm">Winning mobile Number</label>
    <input type="text" class="form-control" id="WinForm" name ="number" placeholder="Enter Mobbile number">
  </div>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token -%>
  <button type="submit" class="btn btn-default">Submit</button>
</form>